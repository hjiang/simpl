(assert
 (> (+ 2 3.0) (* 1.5 2))
 "mixing integers and floats should work")

(assert
 (= "foobar" (+ "foo" "bar"))
 "string concatenation")

(assert
 (> (+ 3.0 2) (+ 1.5 2.0))
 "mixing integers and floats")

(assert (< (/ 3.0 2) (/ 15 2.0)) "division")

(assert
 (= (- 3.0 2) (- 3 2.0))
 "floating point equality")

(assert
 (= (- 3.0 2.0) (- 3 2.0))
 "floating point equality")

(assert
 (= (/ 12.0 2.0) (* 3.0 2.0))
 "floating point equality")
